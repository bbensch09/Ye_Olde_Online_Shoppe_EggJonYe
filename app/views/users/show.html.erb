


<div class="row">
  <div class="product small-12 medium-4 large-4 columns">
    <img src="data:image/jpeg;base64,">
  </div>
  <div class="product small-12 medium-8 large-8 columns">

    <p>
      <strong> Email:</strong>
      <%= @user.email %>
    </p>

    <p>
      <strong> Logged in from:</strong>
      <%= @user.current_sign_in_ip %>
    </p>

    <% if current_user.admin?  %>
      <p>
        <strong> Admin?</strong>
        <%= @user.admin? %>
      </p>

    <% end %>
  </div>
</div>
<hr>
<div class="add_margin clear">
  <%= link_to 'Edit', edit_user_path(@user), :class => "button" %>
</div>
