<h2><%= @category.name %> </h2>
<% if admin?  %>
    <%= link_to 'Edit', edit_category_path(@category), :class => "crud-link" %>
    <%= link_to 'Back to All Categories', categories_path,  :class => "crud-link" %>
<% end %>


<div class="row">
    <%= render partial: 'shared/product_list', locals: {products: @products} %>
</div>

</div>
